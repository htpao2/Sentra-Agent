# 是否启用插件
PLUGIN_ENABLED=true

# 插件超时时间，毫秒
PLUGIN_TIMEOUT_MS=150000

# 思维导图工具 LLM 的 API 基础地址（默认 https://yuanplus.chat/v1）
MINDMAP_BASE_URL=https://yuanplus.chat/v1

# 思维导图工具 LLM 的 API 密钥
MINDMAP_API_KEY=

# 思维导图工具使用的 LLM 模型
MINDMAP_MODEL=gemini-flash-latest

# 思维导图渲染的超时时间（毫秒）
MINDMAP_TIMEOUT=90000

# 思维导图渲染的宽度（像素）
MINDMAP_WIDTH=2400

# 思维导图渲染的高度（像素）
MINDMAP_HEIGHT=1600

# 思维导图加载完成后的等待时间（毫秒）
MINDMAP_WAIT_TIME=9000

# 思维导图的默认样式
MINDMAP_DEFAULT_STYLE=default

# 思维导图工具使用的资源加载模式（'cdn' 或 'local'）
MINDMAP_ASSET_MODE=local

# 思维导图内容在视口中的占比（0-1）
MINDMAP_FIT_RATIO=0.9

# 思维导图最大缩放倍数
MINDMAP_MAX_SCALE=2.0