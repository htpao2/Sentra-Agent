# 是否启用插件
PLUGIN_ENABLED=true

# 插件超时时间，毫秒
PLUGIN_TIMEOUT_MS=150000

# WebSocket SDK (ws_rpc) 的 URL 地址
WS_SDK_URL=ws://localhost:6702

# WebSocket SDK 调用超时时间（毫秒）
WS_SDK_TIMEOUT_MS=15000

# WebSocket SDK 调用参数样式（'object' 或 'pair'）
WS_SDK_ARG_STYLE=pair

# WebSocket SDK 用于发送消息的路径（main + specific）
WS_SDK_SEND_PATH=
WS_SDK_SEND_PATH_PRIVATE=send.private
WS_SDK_SEND_PATH_GROUP=send.group

# 网易云音乐搜索结果数量限制
MUSIC163_SEARCH_LIMIT=6

# 网易云音乐 MUSIC_U Cookie（可选，用于获取更好的播放 URL）
MUSIC163_COOKIE_MUSIC_U=