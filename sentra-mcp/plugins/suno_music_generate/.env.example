# 是否启用插件
PLUGIN_ENABLED=true

# 插件超时时间，毫秒
PLUGIN_TIMEOUT_MS=150000

# SUNO 音乐生成 API 的基础地址（OpenAI 格式）
SUNO_API_BASE_URL=https://yuanplus.chat/v1

# SUNO 音乐生成 API 的密钥
SUNO_API_KEY=your-api-key-here

# SUNO 音乐生成使用的模型名称
SUNO_MODEL=suno-v5-free

# SUNO 音乐生成请求的超时时间（毫秒）
SUNO_TIMEOUT_MS=360000

# 默认的音乐封面图片链接
SUNO_DEFAULT_COVER_URL=https://filesystem.site/cdn/20251003/lSl1Vi7WNkZzBm6lNJ3LxvxkaHJ77M.png

# WebSocket SDK 的地址（用于发送音乐卡片）
WS_SDK_URL=ws://127.0.0.1:6702

# WebSocket SDK 请求的超时时间（毫秒）
WS_SDK_TIMEOUT_MS=15000

# WebSocket SDK 参数风格（如 'pair'）
WS_SDK_ARG_STYLE=pair

# WebSocket SDK 通用发送路径前缀（留空则使用私聊/群聊专用路径）
WS_SDK_SEND_PATH=

# WebSocket SDK 私聊发送路径
WS_SDK_SEND_PATH_PRIVATE=send.private

# WebSocket SDK 群聊发送路径
WS_SDK_SEND_PATH_GROUP=send.group

# 是否启用流式输出
SUNO_STREAM_ENABLED=true